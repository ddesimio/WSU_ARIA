<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffffff"
    tools:context="mwdindustries.aria.scheduleActivity"
    android:orientation="vertical">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/schedule"
        android:gravity="center"
        android:id="@+id/textView"
        android:textAlignment="center"
        android:textColor="#ff000000"
        android:background="#ffa0a0a0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/Building"
        android:id="@+id/textView2"
        android:textColor="#ff000000"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="30dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/Room"
        android:id="@+id/roomNum"
        android:layout_gravity="right|top"
        android:textColor="#ff000000"
        android:layout_marginRight="60dp"
        android:layout_marginTop="30dp"/>


    <!--<Button-->
        <!--android:id="@+id/btnSubmit"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:text="@string/Submit"-->
        <!--android:layout_gravity="center_horizontal|bottom"-->
        <!--android:textColor="#ff000000" />-->

    <Spinner
        android:id="@+id/spinner1"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:entries="@array/buildingChoices"
        android:prompt="@string/Building1"
        android:layout_gravity="left|top"
        android:spinnerMode="dropdown"
        android:layout_marginTop="73dp"
        android:background="#ff000000" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:entries="@array/buildingChoices"
        android:prompt="@string/Building2"
        android:layout_gravity="left|top"
        android:spinnerMode="dropdown"
        android:layout_marginTop="133dp"
        android:background="#ff000000" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:entries="@array/buildingChoices"
        android:prompt="@string/Building3"
        android:layout_gravity="left|top"
        android:spinnerMode="dropdown"
        android:layout_marginTop="193dp"
        android:background="#ff000000" />

    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:entries="@array/buildingChoices"
        android:prompt="@string/Building4"
        android:layout_gravity="left|top"
        android:spinnerMode="dropdown"
        android:layout_marginTop="253dp"
        android:background="#ff000000" />

    <Spinner
        android:id="@+id/spinner5"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:entries="@array/buildingChoices"
        android:prompt="@string/Building5"
        android:layout_gravity="left|top"
        android:spinnerMode="dropdown"
        android:layout_marginTop="313dp"
        android:background="#ff000000" />

    <Spinner
        android:id="@+id/spinner6"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:entries="@array/buildingChoices"
        android:prompt="@string/Building6"
        android:layout_gravity="left|top"
        android:spinnerMode="dropdown"
        android:layout_marginTop="373dp"
        android:background="#ff000000" />

    <EditText
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/Room1"
        android:layout_gravity="right|top"
        android:hint="Room1"
        android:layout_marginTop="60dp"
        android:textColor="@android:color/black" />

    <EditText
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/Room2"
        android:layout_gravity="right|top"
        android:hint="Room2"
        android:layout_marginTop="120dp"
        android:textColor="@android:color/black" />

    <EditText
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/Room3"
        android:layout_gravity="right|top"
        android:hint="Room3"
        android:layout_marginTop="180dp"
        android:textColor="@android:color/black" />

    <EditText
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/Room4"
        android:layout_gravity="right|top"
        android:hint="Room4"
        android:layout_marginTop="240dp"
        android:textColor="@android:color/black" />

    <EditText
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/Room5"
        android:layout_gravity="right|top"
        android:hint="Room5"
        android:layout_marginTop="300dp"
        android:textColor="@android:color/black" />

    <EditText
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/Room6"
        android:layout_gravity="right|top"
        android:hint="Room6"
        android:layout_marginTop="360dp"
        android:textColor="@android:color/black" />

    <!--

    -->

</FrameLayout>




